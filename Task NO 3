#include <iostream>
using namespace std;
int main() {
    int count = 0, num;
    cout << "Enter 7 integers:\n";
    for (int i = 0; i < 7; i++) {
        cin >> num;
        if (num % 5 == 0) {
            count++;
        }
    }
    cout << "There are " << count << " multiples of 5.\n";
    return 0;
}
